               TT= "₹" + str('%.2f'% (Cost_of_Fare +((Cost_of_Fare) *0.09)))
               PaidTax.set (Tax)
               Subtotal.set (ST)
               TotalCost.set (TT)

             if var1.get()==1 and var2.get()==1 and var3.get()==1 and var4.get()==1 and var5.get()==1 and var11.get()== "Kolkata" and var12.get()=="Jaipur" and var13.get()=="4" and var8.get()==1:

