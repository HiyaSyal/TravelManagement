from tkinter import*
from tkinter import ttk
import csv
import random
import time
import tkinter.messagebox
class Travel_registration:
    
     def __init__(self,root):
        self.root=root
        self.root.title("Ticket Reservation")
        self.root.geometry("1350x750+0+0")
        self.root.configure(bg='black')

        DateofOrder=StringVar()
        DateofOrder.set(time.strftime("%d/%m/%Y"))
        Receipt=StringVar()
        Receipt_Ref=StringVar()
        Store=StringVar()
        PaidTax=StringVar()
        Subtotal=StringVar()
        TotalCost=StringVar()

       
        #VARIABLES
        
        var1=IntVar()
        var2=IntVar()
        var3=IntVar()
        var4=IntVar()
        var5=IntVar()
        var6=IntVar()
        var7=IntVar()
        var8=IntVar()
        var9=IntVar()
        var10=IntVar()
        var11=StringVar()
        var12=StringVar()
        var13=StringVar()
       

        Firstname=StringVar()
        Lastname=StringVar()
        Address=StringVar()
        Pincode=StringVar()
        Mobilenumber=StringVar()
        Email=StringVar()

        
        Standard=StringVar()
        Economy=StringVar()
        FirstClass=StringVar()
        Return=StringVar()
        DepartureDate=StringVar()
        AirportTax=StringVar()
        Travel_Ins=StringVar()
        Luggage=StringVar()
        Mile=StringVar()
             
        Standard.set("0")
        Economy.set("0")
        FirstClass.set("0")
        Return.set("0")        
        AirportTax.set("0")
        Travel_Ins.set("0")
        Luggage.set("0")
        Mile.set("0")
        

        #FUNCTIONS FOR COMMANDS 

        def iExit():
            iExit=tkinter.messagebox.askyesno("Travel Ticket Reservation","Do you want to exit?")
            if iExit>0:
                root.destroy()
                return
        def Reset():
            Standard.set("0")
            Economy.set("0")
            FirstClass.set("0")
            Return.set("0")
            
            var1.set(0)
            var2.set(0)
            var3.set(0)
            var4.set(0)
            var5.set(0)
            var6.set(0)
            var7.set(0)
            var8.set(0)
            var9.set(0)
            var11.set("0")
            var12.set("0")
            var13.set(0)
          
            Firstname.set("")
            Lastname.set("")
            Address.set("")
            Pincode.set("")
            Mobilenumber.set("")
            Email.set("")
            DepartureDate.set("")
            
            self.txtReceipt.delete("1.0",END)
            self.cboDeparture.current(0)
            self.cboDestination.current(0)
            self.cboAccomodation.current(1)
            PaidTax.set("")
            Subtotal.set("")
            TotalCost.set("")

            self.txtExt_Luggage.configure(state =DISABLED)
            self.txtTravelling_Insurance.configure(state =DISABLED)
            self.txtAirportTax.configure(state =DISABLED)
            self.txtMile.configure(state =DISABLED)

            self.txtStandard.configure(state =DISABLED)
            self.txtFirstClass.configure(state =DISABLED)
            self.txtEconomy.configure(state =DISABLED)
           

        def Receipt():  
             self.txtReceipt.delete("1.0",END)
             x=random.randint(7429,7583409)
             randomRef=str(x)
             Receipt_Ref.set(randomRef)

             self.txtReceipt.insert(END,'Receipt Ref:\t\t\t\t\t'+Receipt_Ref.get()+"\n")
             self.txtReceipt.insert(END,'Date:\t\t\t\t\t'+DateofOrder.get()+"\n")
             self.txtReceipt.insert(END,'Flight:\t\t\t\t\t'+"Travelling Details \n")
             self.txtReceipt.insert(END,'Firstname:\t\t\t\t\t'+Firstname.get()+"\n")
             self.txtReceipt.insert(END,'Lastname:\t\t\t\t\t'+Lastname.get()+"\n")
             self.txtReceipt.insert(END,'Pincode:\t\t\t\t\t'+Pincode.get()+"\n")
             self.txtReceipt.insert(END,'Address:\t\t\t\t\t'+Address.get()+"\n")
             self.txtReceipt.insert(END,'Mobilenumber:\t\t\t\t\t'+Mobilenumber.get()+"\n")
             self.txtReceipt.insert(END,'Email:\t\t\t\t\t'+Email.get()+"\n")
             self.txtReceipt.insert(END,'Departure:\t\t\t\t\t'+var11.get()+"\n")
             self.txtReceipt.insert(END,'Destination:\t\t\t\t\t'+var12.get()+"\n")
             self.txtReceipt.insert(END,'Departure Date:\t\t\t\t\t'+DepartureDate.get()+"\n")
             self.txtReceipt.insert(END,'Accomodation:\t\t\t\t\t'+var13.get()+"\n")
             self.txtReceipt.insert(END,'Single:\t\t\t\t\t'+str(var6.get())+"\n")
             self.txtReceipt.insert(END,'Return:\t\t\t\t\t'+str(var8.get())+"\n")
             self.txtReceipt.insert(END,'Standard:\t\t\t\t\t'+Standard.get()+"\n")
             self.txtReceipt.insert(END,'Economy:\t\t\t\t\t'+Economy.get()+"\n")
             self.txtReceipt.insert(END,'FirstClass:\t\t\t\t\t'+FirstClass.get()+"\n")
             self.txtReceipt.insert(END,'Paid:\t\t\t\t\t'+PaidTax.get()+"\n")
             self.txtReceipt.insert(END,'Subtotal:\t\t\t\t\t'+str(Subtotal.get())+"\n")
             self.txtReceipt.insert(END,'TotalCost:\t\t\t\t\t'+str(TotalCost.get())+"\n")
         
             

        def Store():
            a=Firstname.get()
            b=Lastname.get()
            c=Pincode.get()
            d=Address.get()
            e=Mobilenumber.get()
            f=Email.get()
            g=Receipt_Ref.get()
            h=DateofOrder.get()
            i=var11.get()
            j=var12.get()
            k=var13.get()
            l=str(var5.get())
            m=str(var7.get())
            n=str(var9.get())
            t=DepartureDate.get()
            u=str(var6.get())
            v=str(var8.get())
                  

            if(a=="" or b=="" or c=="" or d=="" or e=="" or f==""):
                print("Error")
                tkinter.messagebox.showerror("Error","Please enter correct information")
                Firstname.set("")
                Lastname.set("")
                Address.set("")
                Pincode.set("")
                Mobilenumber.set("")
                Email.set("")
                DepartureDate.set("")
                 
            else:
                result=tkinter.messagebox.askquestion("Submit","Are you sure that the data is correct?\n")
                if (result=='yes'):
                     print('here')
                     
                     with open('data4.csv','a') as csvfile:
                            writer=csv.writer(csvfile)
                            
                            writer.writerow([a,b,c,d,e,f,g,h,i,j,k,t,u,v,l,m,n])
                     csvfile.close()

                else:
                    Firstname.set("")
                    Lastname.set("")
                    Address.set("")
                    Pincode.set("")
                    Mobilenumber.set("")
                    Email.set("")
                    DepartureDate.set("")
                    
        def Airport_Tax():
               global Item1
               if (var1.get() == 1):
                    self.txtAirportTax.configure (state= NORMAL)
                    Item1=float (275)
                    AirportTax.set ("₹" + str(Item1))
                    Item1 =AirportTax.get()
               elif var1.get () == 0:
                    self.txtAirportTax.configure (state= DISABLED)
                    AirportTax.set("0")
        
        def Mileage():
               global Item2
               if (var2.get() == 1):
                    self.txtMile.configure (state= NORMAL)
                    Item2=float (23345)
                    Mile.set (Item2)
               elif var2.get () == 0:
                    self.txtMile.configure (state= DISABLED)
                    Mile.set("0")
        

        def Travelling():
               global Item3
               if (var3.get() == 1):
                    self.txtTravelling_Insurance.configure (state= NORMAL)
                    Item3=float (300)
                    Travel_Ins.set ("₹" + str(Item3))
               elif var3.get () == 0:
                    self.txtTravelling_Insurance.configure (state= DISABLED)
                    Travel_Ins.set("0")

        def Lug():
               global Item4
               if (var4.get() == 1):
                    self.txtExt_Luggage.configure (state= NORMAL)
                    Item4=float (600)
                    Luggage.set ("₹" + str(Item4))
               elif var4.get () == 0:
                    self.txtExt_Luggage.configure (state= DISABLED)
                    Luggage.set("0")

        
        def Standard_Fees():
               global Item5
               if (var5.get() == 1):
                    self.txtStandard.configure (state= NORMAL)
                    Item5=float (2500)
                    Standard.set ("₹" + str(Item5))
               elif var5.get () == 0:
                    self.txtStandard.configure (state= DISABLED)
                    Standard.set("0")

        def Economy_Fees():
               global Item6
               if (var7.get() == 1):
                    self.txtEconomy.configure (state= NORMAL)
                    Item6=float (3500)
                    Economy.set ("₹" + str(Item6))
               elif var7.get () == 0:
                    self.txtEconomy.configure (state= DISABLED)
                    Economy.set("0")

        def FirstClass_Fees():
               global Item7
               if (var9.get() == 1):
                    self.txtFirstClass.configure (state= NORMAL)
                    Item7=float (5000)
                    FirstClass.set ("₹" + str(Item7))
               elif var9.get () == 0:
                    self.txtFirstClass.configure (state= DISABLED)
                    FirstClass.set("0")

        
               
#########################################################################################################################################################
                    #LOOP FOR ACCOMODATION=1 AND SINGLE


        def Total_Paid():
             if var1.get()==1 and var2.get()==1 and var3.get()==1 and var4.get()==1 and var5.get()==1 and var11.get()== "Mumbai" and var12.get()=="Chandigarh" and var13.get()=="1" and var6.get()==1:

               q1=float (275)
               q2 =float (300)
               q3 =float (600)
               q4 =float (2500)
               Cost_of_Fare =q1 + q2 + q3+ q4
               Tax= "₹" + str('%.2f'%(( Cost_of_Fare) *0.09))
               ST="₹" + str('%.2f'%(( Cost_of_Fare)))
               TT= "₹" + str('%.2f'% (Cost_of_Fare +((Cost_of_Fare) *0.09)))
               PaidTax.set (Tax)
               Subtotal.set (ST)
               TotalCost.set (TT)

             elif var1.get()==1 and var2.get()==1 and var3.get()==1 and var4.get()==1 and var7.get()==1 and var11.get()== "Mumbai" and var12.get ()=="Chandigarh" and var13.get()=="1" and var6.get()==1:

               q1=float (275)
               q2 =float (300)
               q3 =float (600)
               q4 =float (3500)
               Cost_of_Fare =q1+ q2 + q3+ q4
               Tax= "₹" + str('%.2f'%(( Cost_of_Fare) *0.09))
               ST="₹" + str('%.2f'%(( Cost_of_Fare)))
               TT= "₹" + str('%.2f'% (Cost_of_Fare +((Cost_of_Fare) *0.09)))
               PaidTax.set (Tax)
               Subtotal.set (ST)
               TotalCost.set (TT)


             elif var1.get()==1 and var2.get()==1 and var3.get()==1 and var4.get()==1 and var9.get()==1 and var11.get()== "Mumbai" and var12.get()=="Chandigarh" and var13.get()=="1" and var6.get()==1:

