        MainFrame=Frame(self.root)
        MainFrame.grid()

        Tops=Frame(MainFrame,bd=20,width=1000,relief=RIDGE)
        Tops.pack(side=TOP)
        self.lblTitle=Label(Tops,font=("Comic Sans MS",70,"bold"),text=" Travel Ticket Reservation ",bg='yellow')
        self.lblTitle.grid()

        CustomerDetailsFrame=Frame(MainFrame,width=1350,height=500,bd=20,pady=5,relief=RIDGE,bg='red')
        CustomerDetailsFrame.pack(side=BOTTOM)

        FrameDetails=Frame(CustomerDetailsFrame,width=880,height=400,bd=10,relief=RIDGE,bg='blue')
        FrameDetails.pack(side=LEFT)

        CustomerName=LabelFrame(FrameDetails,width=150,height=250,bd=10, font=("Comic Sans MS",12,'bold'),text='Customer Name',relief=RIDGE,bg='orange',fg='white')
        CustomerName.grid(row=0,column=0)

        TravelFrame=LabelFrame(FrameDetails,width=300,height=250,bd=10,font=("Comic Sans MS",12,'bold'),text='Travel Details',relief=RIDGE)
        TravelFrame.grid(row=0,column=1)

        Ticket_Frame=LabelFrame(FrameDetails,width=300,height=150,relief=FLAT)
        Ticket_Frame.grid(row=1,column=0)
        
        Receipt_ButtonFrame=Frame(CustomerDetailsFrame,width=450,height=400,bd=10,relief=RIDGE,bg='orange')
        Receipt_ButtonFrame.pack(side=RIGHT)

        ReceiptFrame=LabelFrame(Receipt_ButtonFrame,width=350,height=300,font=("Comic Sans MS",12,'bold'),text='Receipt',relief=RIDGE,bg='grey')
